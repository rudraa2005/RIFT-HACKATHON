{
  "name": "rift-hackathon-dev-runner",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -n backend,frontend -c blue,green \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "python backend/run.py",
    "dev:frontend": "npm --prefix frontend run dev -- --host 0.0.0.0 --port 5173"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
